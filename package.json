{
  "name": "photon-tg",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run prestart && next dev",
    "build": "npm run prestart && next build",
    "start": "npm run prestart && next start",
    "prestart": "npm run graphql:generate",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "lint:fix": "next lint --fix",
    "prettier": "prettier src/",
    "prettier:fix": "prettier src/ --write",
    "graphql:generate": "graphql-codegen",
    "graphql:watch": "graphql-codegen --watch --overwrite",
    "i18n:extract": "formatjs extract 'src/**/*.{ts,tsx}' --ignore 'src/**/*.d.ts' --throws --out-file src/model/localization/translations/collected/en.json",
    "i18n:compile": "formatjs compile-folder src/model/localization/translations/collected src/model/localization/translations/compiled",
    "i18n": "yarn i18n:extract && yarn i18n:compile"
  },
